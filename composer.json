{
	"name": "pablograna/mediawiki-extension",
	"type": "library",
	"description": "MediaWiki extension which renders swagger spec in html",
	"keywords": [
		"uml",
		"mediawiki"
	],
	"homepage": "https://github.com/pablograna/swagger-mediawiki",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Pablo Gra\~na",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.0"
	},
	"autoload": {
		"psr-4": {
			"Swagger\\": "src/"
		},
		"files" : [
			"Swagger.php"
		],
		"classmap": [
			"tests/"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.0.6-dev"
		}
	},
	"replace": {
		"mediawiki/swagger-extension": "*"
	}
}

